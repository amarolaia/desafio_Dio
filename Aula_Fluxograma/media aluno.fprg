    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="nota1, nota2, nota3, nota4, soma, media" type="Real" array="False" size=""/>
            <declare name="nome" type="String" array="False" size=""/>
            <output expression="&quot;Insir&#225; nome do aluno(a): &quot;" newline="True"/>
            <input variable="nome"/>
            <output expression="&quot;insira a Nota 1 do Aluno(a) &quot; &amp; nome" newline="True"/>
            <input variable="nota1"/>
            <output expression="&quot;insira a Nota 2 do Aluno(a) &quot; &amp; nome" newline="True"/>
            <input variable="nota2"/>
            <output expression="&quot;insira a Nota 3 do Aluno(a) &quot; &amp; nome" newline="True"/>
