            <output expression="&quot;A m&#233;dia do Aluno(a) &quot; &amp; nome" newline="True"/>
            <output expression="media" newline="True"/>
            <if expression="media&gt;=6.5">
                <then>
                    <output expression="nome &amp; &quot;, PARAB&#201;NS, VOC&#202; EST&#193; APROVADO(A)&quot;" newline="True"/>
                </then>
                <else>
                    <output expression="nome &amp; &quot;, INFELIZMENTE,VOC&#202; EST&#193; REPROVADO(A)&quot;" newline="True"/>
                </else>
            </if>
